{
  "extends": [
    "plugin:@nrwl/nx/react",
  "../../../.eslintrc.json"
],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-template-curly-in-string": "off",
        "no-case-declarations": "off",
        "array-callback-return": "off",
        "no-restricted-globals": "off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {
        "array-callback-return": "off",
        "no-constant-condition": "off"
      }
    }
  ]
}
