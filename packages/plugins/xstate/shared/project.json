  "sourceRoot": "packages/plugins/xstate/shared/src",
  "projectType": "library",
  "implicitDependencies": [
    "@magickml/core"
  ],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/plugins/xstate/shared/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/plugins/xstate/shared/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}