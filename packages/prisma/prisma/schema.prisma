generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model content_objects {
  id          Int       @id @default(autoincrement())
  title       String
  description String
  is_included Boolean   @default(true)
  document_id Int
  documents   documents @relation(fields: [document_id], references: [id], onDelete: Cascade)
}

model documents {
  id              Int               @id @default(autoincrement())
  title           String
  description     String
  is_included     Boolean           @default(true)
  store_id        Int
  content_objects content_objects[]
  documents_store documents_store   @relation(fields: [store_id], references: [id], onDelete: Cascade)
}

model documents_store {
  id        Int         @id @default(autoincrement())
  name      String
  documents documents[]
}

model entities {
  id         Int      @id @default(autoincrement())
  dirty      Boolean?
  enabled    Boolean?
  updated_at String?
  spells     spells[] @relation("entitiesTospells")
  data       String?
}

model events {
  id      Int     @id @default(autoincrement())
  type    String?
  agent   String?
  client  String?
  channel String?
  sender  String?
  text    String?
  date    String?
  speaker String?
}

model spells {
  id         String     @id @unique
  name       String     @unique
  graph      String?
  created_at DateTime?
  updated_at DateTime?
  deleted_at DateTime?
  modules    String?
  gameState  String?
  entities   entities[] @relation("entitiesTospells")
}
