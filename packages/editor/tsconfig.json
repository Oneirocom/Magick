{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "rootDir": "../..",
    "outDir": "lib",
    "strict": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite/client"]
  },
  "files": [],
  "include": [
    "src",
    "../core/client/src/providers/ConfigProvider.tsx",
    "../core/client/src/providers/PubSubProvider.tsx",
    "../core/client/src/providers/FeathersProvider.tsx"
, "../core/shared/src/state"  ]
}
