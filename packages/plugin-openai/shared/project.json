{
  "name": "@magickml/plugin-search-shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugin-search/shared/src",
  "projectType": "library",
  "implicitDependencies": ["@magickml/engine"],
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "configFile": "packages/plugin-search/shared/vite.config.ts",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/plugin-search/shared"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/plugin-search/shared/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/plugin-search/shared/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
