{
  "name": "agent-module",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/modules/agent/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "mkdir -p ./dist/packages/modules/agent",
          "touch ./dist/packages/modules/agent/.gitkeep",
          "cd packages/modules/agent && pwd && npx unbuild",
          "cp -r ./packages/modules/agent/* ./dist/packages/modules/agent"
        ],
        "parallel": false
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "packages/modules/agent"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/modules/agent/jest.config.ts"
      }
    }
  }
}
