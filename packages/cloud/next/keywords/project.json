{
  "name": "keywords",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cloud/next/keywords/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/cloud/next/keywords",
        "tsConfig": "packages/cloud/next/keywords/tsconfig.lib.json",
        "packageJson": "packages/cloud/next/keywords/package.json",
        "main": "packages/cloud/next/keywords/src/index.ts",
        "assets": ["packages/cloud/next/keywords/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/cloud/next/keywords/jest.config.ts"
      }
    }
  },
  "tags": []
}
