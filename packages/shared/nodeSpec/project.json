{
  "name": "shared/nodeSpec",
  "implicitDependencies": ["plugin/*"],
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/nodeSpec/src",
  "projectType": "library",
  "targets": {
    "build": {
      "dependsOn": ["^build"],
      "executor": "@magickml/buildSpec:buildSpec",
      "options": {
        "specFileLocation": "packages/shared/nodeSpec/src",
        "specFileName": "nodeSpec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shared/nodeSpec/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/shared/nodeSpec/jest.config.ts"
      }
    }
  },
  "tags": []
}
