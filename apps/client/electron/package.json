  "devDependencies": {
    "electron": "^19.0.0",
    "electron-builder": "~22.14.13",
    "electron-rebuild": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
