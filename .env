# Default settings, override in .env.local file that is auto-generated on install

# Database
### sqlite (default)
DATABASE_TYPE=sqlite3
# sqlite file path, absolute or relative path
DATABASE_URL=./mydb.sqlite
### postgres
# DATABASE_TYPE=pg
# DATABASE_URL=postgres://user:password@db.dbname.supabase.co:5432/postgres

CLIENT_PLUGINS=discord,rest,loop,finetune,ethereum
SERVER_PLUGINS=discord,rest,loop,finetune,ethereum

# if you want to override OpenAI's endpoint with another URL that matches the same API
VITE_OPENAI_ENDPOINT=https://api.openai.com/v1

# OpenAI
OPENAI_API_KEY=

# Banana.dev
BANANA_API_KEY=
BANANA_MODEL_KEY=

SINGLE_USER_MODE=true
VITE_SINGLE_USER_MODE=true

# Memory to use for build cache
NODE_OPTIONS="--max-old-space-size=8192"

# Server settings
PAGINATE_DEFAULT=100
PAGINATE_MAX=1000

# Leave blank - override in .env.local file that is auto-generated on install
PROJECT_ID=
