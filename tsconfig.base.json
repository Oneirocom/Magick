{
  "compilerOptions": {
    "rootDir": ".",
    "resolveJsonModule": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2021", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "inlineSources": true,
    "noPropertyAccessFromIndexSignature": false,
    "strict": false,
    "typeRoots": [
      "packages/@types",
      "./node_modules/@types",
      "./node_modules",
      "/node_modules/@types",
      "portal/cloud/packages/types/src/lib/next-auth"
    ],
    "paths": {
      "@magickml/agent": ["apps/agent/src/index.ts"],
      "@magickml/client": ["apps/client/src/index.ts"],
      "@magickml/docs": ["apps/docs/src/index.ts"],
      "@magickml/feathersRedux": [
        "packages/client/feathers-redux-toolkit/src/index.ts"
      ],
      "@magickml/portal-api": ["portal/cloud/packages/api/src/index.ts"],
      "@magickml/portal-api-client": [
        "portal/cloud/packages/api-client/src/index.ts"
      ],
      "@magickml/portal-auth": ["portal/cloud/packages/auth/src/index.ts"],
      "@magickml/portal-billing": [
        "portal/cloud/packages/billing/src/index.ts"
      ],
      "@magickml/portal-chat": ["portal/cloud/packages/chat/src/index.ts"],
      "@magickml/portal-config": ["portal/cloud/packages/config/src/index.ts"],
      "@magickml/portal-db": ["portal/cloud/packages/db/src/index.ts"],
      "@magickml/portal-hooks": ["portal/cloud/packages/hooks/src/index.ts"],
      "@magickml/portal-layout": ["portal/cloud/packages/layout/src/index.ts"],
      "@magickml/portal-mail": ["portal/cloud/packages/mail/src/index.ts"],
      "@magickml/portal-pages": ["portal/cloud/packages/pages/src/index.ts"],
      "@magickml/portal-providers": [
        "portal/cloud/packages/providers/src/index.ts"
      ],
      "@magickml/portal-state": ["portal/cloud/packages/state/src/index.ts"],
      "@magickml/portal-types": ["portal/cloud/packages/types/src/index.ts"],
      "@magickml/portal-ui": ["portal/cloud/packages/ui/src/index.ts"],
      "@magickml/portal-utils-server": [
        "portal/cloud/packages/utils/server/src/index.ts"
      ],
      "@magickml/portal-utils-shared": [
        "portal/cloud/packages/utils/shared/src/index.ts"
      ],
      "@magickml/providers": ["packages/client/providers/src/index.ts"],
      "@magickml/server": ["apps/server/src/index.ts"],
      "@magickml/server-db": ["packages/server/db/src/index.ts"],
      "@magickml/types": ["packages/@types/src/index.ts"],
      "client/core": ["packages/client/core/src/index.ts"],
      "client/editor": ["packages/client/editor/src/index.ts"],
      "client/feathers-client": [
        "packages/client/feathers-client/src/index.ts"
      ],
      "client/layouts": ["packages/client/layouts/src/index.ts"],
      "client/magickml-chatbox": [
        "packages/client/magickml-chatbox/src/index.ts"
      ],
      "client/state": ["packages/client/state/src/index.ts"],
      "@magickml/client-ui": ["packages/client/ui/src/index.ts"],
      "clientConfig": ["packages/client/config/src/index.ts"],
      "communication": ["packages/shared/communication/src/index.ts"],
      "plugin/core": ["plugins/core/src/index.ts"],
      "plugins/shared": ["plugins/shared/src/index.ts"],
      "server/agents": ["packages/server/agents/src/index.ts"],
      "server/cloud-agent-manager": [
        "packages/server/cloud-agent-manager/src/index.ts"
      ],
      "server/cloud-agent-worker": [
        "packages/server/cloud-agent-worker/src/index.ts"
      ],
      "server/command-hub": ["packages/server/command-hub/src/index.ts"],
      "server/communication": ["packages/server/communication/src/index.ts"],
      "server/core": ["packages/server/core/src/index.ts"],
      "server/credentials": ["packages/server/credentials/src/index.ts"],
      "server/event-tracker": ["packages/server/event-tracker/src/index.ts"],
      "server/grimoire": ["packages/server/grimoire/src/index.ts"],
      "server/logger": ["packages/server/logger/src/index.ts"],
      "server/plugin": ["packages/server/plugin/src/index.ts"],
      "server/pluginManager": ["packages/server/pluginManager/src/index.ts"],
      "server/redis-pubsub": ["packages/server/redis-pubsub/src/index.ts"],
      "server/schemas": ["packages/server/schemas/src/index.ts"],
      "servicesShared": ["packages/shared/servicesShared/src/index.ts"],
      "shared/config": ["packages/shared/config/src/index.ts"],
      "shared/core": ["packages/shared/core/src/index.ts"],
      "shared/nodeSpec": ["packages/shared/nodeSpec/src/index.ts"],
      "shared/rete": ["packages/shared/rete/src/index.ts"],
      "shared/utils": ["packages/shared/utils/src/index.ts"],
      "stylesheets": ["packages/client/stylesheets/src/index.ts"]
